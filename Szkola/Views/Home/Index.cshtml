<br>
Jasnogórska 4<br>
31-000 Kraków<br>

<!--The div element for the map -->
<div id="map"></div>
<script>
    // Initialize and add the map
    function initMap() {
        // The location of Jasnogórska
        var jasnogorska = { lat: 50.0914689, lng: 19.894032 };
        // The map, centered at jasnogorska
        var map = new google.maps.Map(
            document.getElementById('map'), { zoom: 14, center: jasnogorska });
        // The marker, positioned at jasnogorska
        var marker = new google.maps.Marker({ position: jasnogorska, map: map });
    }

</script>
<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyB3uhI44V6AwRUMcwAp_iJotWoJN0pQHFk&callback=initMap"
        async defer></script>

